{
  "heex if else": {
    "prefix": "h_ifelse",
    "body": ["<%= if $1 do %>", "  $2", "<% else %>", "  $3", "<% end %>"]
  },
  "heex if": {
    "prefix": "h_if",
    "body": [" <%= if $1 do %>", "   $2", "<% end %>"]
  },

  "heex for": {
    "prefix": "h_if",
    "body": [" <%= for item <- items do %>", "   $2", "<% end %>"]
  }
}
